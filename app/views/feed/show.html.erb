<h1><%= @feed.name %> Feed</h1>
<div>
  <% @feed.question_banks.each do |bank| %>
    <% question = bank.question %>
    <div>
      <h2><%= link_to question.entry, question_show_path(question) %></h2>
      <ul>
        <% question.answers.each do |answer| %>
          <li><%= answer.entry %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
</div>