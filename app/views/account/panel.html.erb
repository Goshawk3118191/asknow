<h1>How's it going, <%= @account.name %>?</h1>
<% if !@account.guest? %>
  <%= link_to "Logout", session_logout_path, method: "post" %>
<% else %>
 <%= link_to "Login", session_login_path %>
<% end %>
<p>Your current community is <%= current_account.community.name %></p>
